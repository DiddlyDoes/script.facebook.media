	<onload lang="python"><![CDATA[
print "LOAD"
if CLOSEREADY:
	try:
		mc.GetActiveWindow().ClearStateStack()
		session.loadOptions()
		session.restoreState(session.current_state)
		print "FACEBOOK MEDIA: STATE RESTORED"
	except:
		import traceback
		traceback.print_exc()
else:
		session = FacebookSession()
CLOSEREADY = True
]]></onload>
<onunload lang="python"><![CDATA[
print "UNLOAD"
mc.GetActiveWindow().ClearStateStack()
session.setFriend('')
CLOSEREADY = True
]]></onunload>
